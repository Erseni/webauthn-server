{
    "name": "madwizard/webauthn",
    "description": "Web Authentication API server for PHP",
    "type": "library",
    "autoload": {
        "psr-4": {
            "MadWizard\\WebAuthn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadWizard\\WebAuthn\\Tests\\": "tests/",
            "MadWizard\\WebAuthn\\Conformance\\": "conformance/src/"
        }
    },
    "require": {
        "php": "^7.2.0",
        "sop/x509": "^0.7.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "psr/cache": "^1.0",
        "kevinrob/guzzle-cache-middleware": "^3.3",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/cache": "^4.4",
        "phpstan/phpstan": "^0.11.9",
        "symfony/var-dumper": "^5.1",
        "symfony/dotenv": "^5.1"
    },
    "archive": {
        "exclude": [
            "/tests",
            "/phpunit.xml.dist",
            "/conformance"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Bleeker",
            "email": "support@madwizard.org"
        }
    ]
}
